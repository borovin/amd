{
  "name": "@basket/amd",
  "version": "2.1.1",
  "description": "minimal AMD loader for browsers",
  "main": "loader.js",
  "directories": {
    "test": "tests"
  },
  "bin": {
    "amd": "./bin/amd.js"
  },
  "scripts": {
    "test": "codeceptjs run --steps",
    "start": "serve static",
    "build": "npm run build:loader && npm run build:static",
    "build:static": "node ./tools/build.js",
    "build:loader": "babel loader.js -o loader.min.js"
  },
  "author": "a@borovin.com",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-babili": "0.0.12",
    "babel-preset-es2015": "^6.22.0",
    "codeceptjs": "^0.5.0",
    "nightmare": "^2.10.0",
    "nightmare-upload": "^0.1.1",
    "serve": "^3.4.0"
  },
  "dependencies": {
    "babel-core": "^6.23.1",
    "babel-plugin-transform-es2015-modules-amd": "^6.22.0",
    "commander": "^2.9.0",
    "detective-amd": "^2.4.0",
    "fs-extra": "^2.0.0",
    "globule": "^1.1.0",
    "lodash": "^4.17.4",
    "resolve-cwd": "^1.0.0"
  }
}
